<?xml version="1.0" encoding="utf-8" ?>
<category title="Command Prompt">
	<shortcut description="Enter Mark mode">
		<key>CTRL</key>
		<key>M</key>
	</shortcut>
	<shortcut description="Begin block selection">
		<key>ALT</key>
		<key>SELECTION KEY</key>
	</shortcut>
	<shortcut description="Move cursor left">
		<key>LEFT</key>
	</shortcut>
	<shortcut description="Move cursor right">
		<key>RIGHT</key>
	</shortcut>
	<shortcut description="Move cursor up">
		<key>UP</key>
	</shortcut>
	<shortcut description="Move cursor down">
		<key>DOWN</key>
	</shortcut>
	<shortcut description="Move cursor up one page">
		<key>PAGE UP</key>
	</shortcut>
	<shortcut description="Move cursor down one page">
		<key>PAGE DOWN</key>
	</shortcut>
	<shortcut description="Move to start of buffer (Mark mode)">
		<key>CTRL</key>
		<key>HOME</key>
	</shortcut>
	<shortcut description="Move to end of buffer (Mark mode)">
		<key>CTRL</key>
		<key>END</key>
	</shortcut>
	<shortcut description="Move up one line in history">
		<key>CTRL</key>
		<key>UP</key>
	</shortcut>
	<shortcut description="Move down one line in history">
		<key>CTRL</key>
		<key>DOWN</key>
	</shortcut>
	<shortcut description="Move viewport to top">
		<key>CTRL</key>
		<key>HOME</key>
	</shortcut>
	<shortcut description="Move viewport to command line">
		<key>CTRL</key>
		<key>END</key>
	</shortcut>
</category>