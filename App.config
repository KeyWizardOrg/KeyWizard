			<add key="Down arrow" action="Move the cursor down" function="arrowDown"/>
			<add key="Page up" action="Move the cursor by one page up" function="pageUp"/>
			<add key="Page down" action="Move the cursor by one page down" function ="pageDown"/>
			<add key="Ctrl + Home (Mark mode)" action="Move the cursor to the beginning of the buffer" function="ctrlHomeMarkMode"/>
			<add key="Ctrl + End (Mark mode)" action="Move the cursor to the end of the buffer" function ="ctrlEndMarkMode"/>
			<add key="Ctrl + Up arrow" action="Move up one line in the output history" function="ctrlUp"/>
			<add key="Ctrl + Down arrow" action="Move down one line in the output history" function="ctrlDown"/>
			<add key="Ctrl + Home (History navigation)" action="If the command line is empty, move the viewport to the top of the buffer. Otherwise, delete all the characters to the left of the cursor in the command line"
				 function ="ctrlHomeHistoryNav"/>
			<add key="Ctrl + End (History navigation)" action="If the command line is empty, move the viewport to the command line. Otherwise, delete all the characters to the right of the cursor in the command line"
				 function ="ctrlEndHistoryNav"/>
		</Command_Prompt>
