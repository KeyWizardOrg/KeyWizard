			<add key="Alt + P" action="Show or hide the preview pane"/>
			<add key="Alt + Right arrow" action="View the next folder"/>
			<add key="Alt + Shift + P" action="Show or hide the details pane"/>
			<add key="Alt + Up arrow" action="Move up a level in the folder path"/>
			<add key="Ctrl + Arrow key (to move to an item) + Spacebar" action="Select multiple individual items"/>
			<add key="Ctrl + D" action="Delete the selected item and move it to the Recycle Bin"/>
			<add key="Delete" action="Delete the selected item and move it to the Recycle Bin"/>
			<add key="Ctrl + E" action="Select the search box"/>
			<add key="Ctrl + F" action="Select the search box"/>
			<add key="Ctrl + L" action="Focus on the address bar"/>
			<add key="Ctrl + mouse drag a file" action="When you drop the file, create a copy in that location"/>
			<add key="Ctrl + mouse scroll wheel" action="Change the size and appearance of file and folder icons"/>
			<add key="Ctrl + N" action="Open a new window"/>
			<add key="Ctrl + Number (1-9)" action="Move to that tab number"/>
			<add key="Ctrl + Plus (+)" action="Resize all columns to fit text"/>
			<add key="Ctrl + Shift + E" action="Expands all folders from the tree in the navigation pane"/>
			<add key="Ctrl + Shift + N" action="Create a new folder"/>
			<add key="Ctrl + Shift + Number (1-9)" action="Change the view style"/>
			<add key="Ctrl + Shift + Tab" action="Move to the previous tab"/>
			<add key="Ctrl + T" action="Open a new tab and switch to it"/>
			<add key="Ctrl + W" action="Closes the active tab. Closes the window if there's only one tab open"/>
			<add key="Ctrl + Tab" action="Move to the next tab"/>
			<add key="End" action="Scroll to the bottom of the active window"/>
			<add key="F2" action="Rename the selected item"/>
			<add key="F3" action="Search for a file or folder"/>
			<add key="F4" action="Select the address bar to change the current path"/>
			<add key="F5" action="Refresh the window"/>
			<add key="F6" action="Cycle through elements in the window"/>
			<add key="F11" action="Maximize or minimize the active window"/>
			<add key="Home" action="Scroll to the top of the active window"/>
			<add key="Left arrow" action="Collapse the current selection or select the folder that the folder was in"/>
			<add key="Right arrow" action="Display the current selection or select the first subfolder"/>
			<add key="Shift + Arrow keys" action="Select multiple items"/>
			<add key="Shift + Delete" action="Permanently delete the selected item without moving it to the Recycle Bin"/>
			<add key="Shift + F10" action="Display the context menu for the selected item"/>
			<add key="Shift + mouse drag a file" action="When you drop the file, move it to that location"/>
			<add key="Shift + mouse right-click" action="Display the Show more options context menu for the selected item"/>
			<add key="Windows key + E" action="Open File Explorer"/>
		</File_Explorer>
		<Multiple_Desktops>
			<add key="Windows key + Tab" action="Open Task view"/>
			<add key="Windows key + Ctrl + D" action="Create another desktop"/>
			<add key="Windows key + Ctrl + Right arrow" action="Switch between other desktops you've created on the right"/>
			<add key="Windows key + Ctrl + Left arrow" action="Switch between other desktops you've created on the left"/>
			<add key="Windows key + Ctrl + F4" action="Close the desktop you're using"/>
		</Multiple_Desktops>
		<Taskbar>
			<add key="Alt + Shift + Arrow" action="When an app icon is in focus, move its position in the direction of the arrow"/>
			<add key="Ctrl + select a grouped app icon on the taskbar" action="When multiple windows for the same app are grouped on the taskbar, press Ctrl as you select the taskbar icon to shift focus through the windows in the app group"/>
			<add key="Ctrl + Shift + select an app icon on the taskbar" action="Open the app as an administrator"/>
			<add key="Shift + right-click an app icon on the taskbar" action="Show the window menu for the app"/>
			<add key="Shift + right-click a grouped taskbar button" action="Show the window menu for the group"/>
			<add key="Shift + select an app icon on the taskbar" action="Open another instance of the app"/>
			<add key="Windows key + Alt + Enter (on taskbar item focus)" action="Open taskbar settings"/>
			<add key="Windows key + Alt + number (0-9)" action="Open the jump list for the app pinned to the taskbar in the position indicated by the number"/>
			<add key="Windows key + B" action="Set the focus to the first icon in the notification area of the taskbar"/>
			<add key="Windows key + Ctrl + number (0-9)" action="Switch to the last active window of the app pinned to the taskbar in the position indicated by the number"/>
			<add key="Windows key + Ctrl + Shift + number (0-9)" action="Open as an administrator a new instance of the app located at the given position on the taskbar"/>
			<add key="Windows key + number (0-9)" action="Open the app pinned to the taskbar in the position indicated by the number. If the app is already running, switch to that app"/>
			<add key="Windows key + Shift + Number (0-9)" action="Start a new instance of the app pinned to the taskbar in the position indicated by the number"/>
			<add key="Windows key + T" action="Cycle through apps in the taskbar"/>
		</Taskbar>
		<Settings>
			<add key="Arrow keys" action="Move focus through the currently selected region or scroll the page"/>
			<add key="Backspace" action="Navigate backwards"/>
			<add key="Enter" action="Select the current item in focus"/>
			<add key="Spacebar" action="Select the current item in focus"/>
			<add key="Shift + Tab" action="Cycle backward through regions"/>
			<add key="Tab" action="Cycle forward through regions"/>
			<add key="Windows key + I" action="Open the Settings app"/>
		</Settings>
	</appSettings>
</configuration>


