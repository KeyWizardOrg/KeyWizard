			<add key="Alt + Tab" action="Switch between open windows. To cycle through multiple windows, continue to hold the Alt key and press Tab multiple times"
				 function ="altTab"/>
			<add key="Alt + underlined letter" action="For actions in command or context menus, some windows and apps specify keyboard shortcuts by underlining a character in the action name. Press Alt and then the letter to do that action"
				 function ="altUnderlinedLetter"/>
			<add key="Ctrl + A" action="Select all items in a window"
				 function="ctrlA"/>
			<add key="Ctrl + Alt + Del" action="Switch to the security screen where you can lock the desktop, switch user, sign out, change a password, or open Task Manager"
