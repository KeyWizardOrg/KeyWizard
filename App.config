			<add key="Alt + P" action="Show or hide the preview pane" function ="altP"/>
			<add key="Alt + Right arrow" action="View the next folder" function ="altRight"/>
			<add key="Alt + Shift + P" action="Show or hide the details pane" function="altShiftP"/>
