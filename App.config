			<add key="Ctrl + F5" action="Refresh the current window" function ="ctrlF5"/>
			<add key="Ctrl + R" action="Refresh the current window" fucntion ="ctrlR"/>
			<add key="Ctrl + Shift" action="When multiple keyboard layouts are available, switch the keyboard layout" fucntion ="ctrlShift"/>
			<!--<add key="Ctrl + Shift + arrow keys" action="When a tile is in focus on the Start menu, move it into another tile to create a folder" function ="ctrlShiftArrowKeys"/>-->
			<add key="Ctrl + Shift + Left arrow" action="Select words backward from the current cursor position" function="ctrlShiftLeft"/>
			<add key="Ctrl + Shift + Right arrow" action="Select words forward from the current cursor position" function="ctrlShiftRight"/>
			<add key="Ctrl + Shift + Esc" action="Open Task Manager" function ="ctrlShiftEsc"/>
			<add key="Ctrl + Spacebar" action="Enable or disable the Chinese input method editor (IME)" function="ctrlSpace"/>
			<add key="Ctrl + Y" action="Redo an action that was previously undone with Ctrl + Z" function ="ctrlY"/>
			<add key="Ctrl + Z" action="Undo the previous action" function ="ctrlZ"/>
			<add key="Esc" action="Stop or leave the current task, or dismiss a dialog box" function ="esc"/>
			<add key="Escape" action="Stop or leave the current task, or dismiss a dialog box" function ="Escape"/>
			<add key="F5" action="Refresh the active window" function ="F5"/>
			<add key="F6" action="Cycle through elements in a window or on the desktop" function ="F6"/>
			<add key="F10" action="Activate the menu bar in the active window" function ="F10"/>
			<add key="PrtSc" action="Select a region of the screen to capture a screenshot to the clipboard. You can then open the Snipping Tool to share or markup the image"
				 function ="PrtSc"/>
		</Desktop>
		<Windows_Key>
			<add key="Windows key" action="Open or close the Start menu" function ="windowsKey"/>
			<add key="Windows key + A" action="Open the Windows 11 action center" function ="windowsKeyA"/>
