			<add key="Ctrl + Alt + Tab" action="View a thumbnail of all open apps. Use the arrow keys to switch between open apps" function ="ctrlAltTab"/>
			<add key="Ctrl + Esc" action="Open the Start menu" function ="ctrlEsc"/>
			<add key="Ctrl + F4" action="In apps that are full-screen and let you have multiple documents open at the same time, close the active document but not the entire app"
