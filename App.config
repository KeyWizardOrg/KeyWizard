			<add key="Shift + Ctrl + Home" action="Select text between the current cursor position and the beginning of the document" function ="shiftCtrlHome"/>
			<add key="Shift + Ctrl + Left" action="Select words backward from the current cursor position" function="shiftCtrlLeft"/>
			<add key="Shift + Ctrl + Right" action="Select words forward from the current cursor position" function="shiftCtrlRight"/>
			<add key="Shift + Ctrl + Up" action="Select paragraphs backward from the current cursor position" function="shiftCtrlUp"/>
			<add key="Shift + Down arrow" action="Select lines forward from the current cursor position" function="shiftDown"/>
			<add key="Shift + End" action="Select text from the current cursor position to the end of the current line" function="shiftEnd"/>
			<add key="Shift + Home" action="Select text from the current cursor position to the beginning of the current line" function="shiftHome"/>
			<add key="Shift + Left arrow" action="Select characters backward from the current cursor position" function="shiftLeft"/>
