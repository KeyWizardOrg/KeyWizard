		<Command_Prompt>
			<add key="Ctrl + M" action="Enter Mark mode" function ="ctrlM"/>
			<add key="Alt + selection key" action="Begin selection in block mode" function="altSelectionKey"/>
			<add key="Left arrow" action="Move the cursor left" function="arrowLeft"/>
