			<add key="Shift + Page Up" action="Select one page of text backward from the current cursor position" function="shiftPageUp"/>
			<add key="Shift + Right arrow" action="Select characters forward from the current cursor position" function="shiftRight"/>
			<add key="Shift + Up arrow" action="Select lines backward from the current cursor position" function="shiftUp"/>
			<add key="Tab" action="Indent the cursor one tab stop" function ="tab"/>
			<add key="Up arrow" action="Move the cursor backward to the previous line" function ="up"/>
		</Text_Editing>
		<Desktop>
			<add key="Alt + A" action="Set focus to the first icon in the Suggested actions menu" function ="altA"/>
			<add key="Alt + Esc" action="Cycle through windows in the order in which they were opened" function="altEsc"/>
