			<add key="Alt + Left arrow" action="Go back" function ="altLeft"/>
			<add key="Alt + Page Down" action="Move down one screen" function="altPageDown"/>
			<add key="Alt + Page Up" action="Move up one screen" function="altPageUp"/>
			<add key="Alt + PrtScn" action="Capture a screenshot of the active window and copy it to the clipboard" function ="altPrtScn"/>
			<add key="Alt + Right arrow" action="Go forward" function ="altRight"/>
			<!--<add key="Alt + Shift + arrow keys" action="When a group or tile is in focus on the Start menu, move it in the specified direction"
				 function ="altShiftArrowKeys"/>-->
			<add key="Alt + Shift + Left arrow" action="Move a group or tile left on the Start menu" function="altShiftLeft"/>
			<add key="Alt + Shift + Right arrow" action="Move a group or tile right on the Start menu" function="altShiftRight"/>
