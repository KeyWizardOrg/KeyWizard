			<add key="PgDn" action="Move the cursor forward by one page" funtion ="pgDn"/>
			<add key="Page up" action="Move the cursor backward by one page" function ="pageUp"/>
			<add key="PgUp" action="Move the cursor backward by one page" funtion="pgUp"/>
			<add key="Right arrow" action="Move the cursor forward to the next character" function="rightArrow"/>
